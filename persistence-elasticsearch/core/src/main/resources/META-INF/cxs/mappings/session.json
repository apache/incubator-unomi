{
  "session": {
    "dynamic_templates": [
      {
        "all": {
          "match": "*",
          "match_mapping_type": "string",
          "mapping": {
            "type": "string",
            "analyzer": "folding"
          }
        }
      }
    ],
    "properties": {
      "duration": {
        "type": "long"
      },
      "itemId": {
        "type": "string",
        "analyzer": "folding"
      },
      "itemType": {
        "type": "string",
        "analyzer": "folding"
      },
      "lastEventDate": {
        "type": "date",
        "format": "strict_date_optional_time||epoch_millis"
      },
      "profileId": {
        "type": "string",
        "analyzer": "folding"
      },
      "properties": {
        "properties": {
          "location": {
            "type": "geo_point"
          }
        }
      },
      "scope": {
        "type": "string",
        "analyzer": "folding"
      },
      "size": {
        "type": "long"
      },
      "systemProperties": {
        "type": "object"
      },
      "timeStamp": {
        "type": "date",
        "format": "strict_date_optional_time||epoch_millis"
      }
    }
  }
}